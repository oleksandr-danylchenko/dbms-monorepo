{
  "extends": "../../.eslintrc.react.json",
  "rules": {
    "react/jsx-props-no-spreading": [
      "error",
      {
        "html": "ignore",
        "custom": "enforce",
        "explicitSpread": "ignore",
        "exceptions": [
          "Section",
          "PublicRoute",
          "Component",
          "Route"
        ]
      }
    ]
  }
}
